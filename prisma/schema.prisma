generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model product {
  id         String   @id @default(uuid())
  name       String
  bar_code   String
  price      Decimal
  created_at DateTime @default(now())

  @@map("products") 
}

